{
  "name": "quaisr-actions",
  "version": "0.1.0",
  "description": "Official GitHub Actions for Quaisr - automate your workflows with the Quaisr platform",
  "private": true,
  "workspaces": [
    "register-block",
    "get-blocks"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
